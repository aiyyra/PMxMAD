<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
xmlns:app="http://schemas.android.com/apk/res-auto"
tools:context=".skills_PM_Fragment">

<ScrollView
    android:id="@+id/scrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="16dp"
    tools:layout_editor_absoluteY="26dp">


        <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


            <TextView
                android:id="@+id/textIntroPM"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:fontFamily="@font/inter_medium"
                android:text="Physics is Fun!"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="MissingConstraints"
                app:layout_constraintHorizontal_bias="0.0" app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="16dp"/>

            <TextView
                android:id="@+id/textPm1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ELECTROMAGNETISM"
                android:fontFamily="@font/inter_bold"
                app:layout_constraintTop_toBottomOf="@+id/textIntroPM" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="12dp"
                app:layout_constraintHorizontal_bias="0.129"/>
            <WebView
                android:id="@+id/cp_vid1"
                android:layout_width="368dp"
                android:layout_height="208dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.465" android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/textPm1"/>

            <CheckBox
                android:id="@+id/checkBox"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:fontFamily="@font/inter_medium"
                android:text="Mark as done"
                android:theme="@style/colored_checkBox"
                app:layout_constraintEnd_toEndOf="@+id/cp_vid1"
                app:layout_constraintTop_toBottomOf="@+id/cp_vid1" />

            <TextView
                android:id="@+id/textPm2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="GRAVITY,WEIGHT AND GPE"
                android:fontFamily="@font/inter_bold"
                app:layout_constraintStart_toStartOf="@+id/textPm1"
                app:layout_constraintTop_toBottomOf="@+id/checkBox" />

            <WebView
                android:id="@+id/cp_vid2"
                android:layout_width="368dp"
                android:layout_height="208dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textPm2" />

            <CheckBox
                android:id="@+id/checkBox2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:fontFamily="@font/inter_medium"
                android:text="Mark as done"
                android:theme="@style/colored_checkBox"
                app:layout_constraintEnd_toEndOf="@+id/cp_vid2"
                app:layout_constraintTop_toBottomOf="@+id/cp_vid2" />

            <TextView
                android:id="@+id/textPm3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="EFFICIENCY"
                android:fontFamily="@font/inter_bold"
                app:layout_constraintStart_toStartOf="@+id/textPm2"
                app:layout_constraintTop_toBottomOf="@+id/checkBox2" />

            <WebView
                android:id="@+id/cp_vid3"
                android:layout_width="368dp"
                android:layout_height="208dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textPm3" />

            <CheckBox
                android:id="@+id/checkBox3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:fontFamily="@font/inter_medium"
                android:text="Mark as done"
                android:theme="@style/colored_checkBox"
                app:layout_constraintEnd_toEndOf="@+id/cp_vid3"
                app:layout_constraintTop_toBottomOf="@+id/cp_vid3" />

            <TextView
                android:id="@+id/textPm4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="POWER AND WORK DONE"
                android:fontFamily="@font/inter_bold"
                app:layout_constraintStart_toStartOf="@+id/textPm3"
                app:layout_constraintTop_toBottomOf="@+id/checkBox3" />

            <WebView
                android:id="@+id/cp_vid4"
                android:layout_width="368dp"
                android:layout_height="208dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textPm4" />

            <CheckBox
                android:id="@+id/checkBox4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="100dp"
                android:fontFamily="@font/inter_medium"
                android:text="Mark as done"
                android:theme="@style/colored_checkBox"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/cp_vid4"
                app:layout_constraintTop_toBottomOf="@+id/cp_vid4" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>


    </androidx.constraintlayout.widget.ConstraintLayout>